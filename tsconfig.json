{
  "compilerOptions": {
      "outDir":"./dist",
      "target":"es5",
      "module":"ESNext",
      "jsx":"react-jsx",
      "noImplicitAny": true,
      "allowSyntheticDefaultImports": true,
      "lib": ["DOM","DOM.Iterable","ESNext"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "baseUrl": ".",
      "paths": {
          "@/*": ["src/*"]
      },
  },
  "include": ["src"]
}